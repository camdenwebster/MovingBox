// Base.xcconfig.template
// Template for generating Base.xcconfig with secrets

// JWT Secret for AI proxy authentication
JWT_SECRET = $(JWT_SECRET)

// RevenueCat API key
REVENUE_CAT_API_KEY = $(REVENUE_CAT_API_KEY)

// Sentry DSN
SENTRY_DSN = $(SENTRY_DSN)

// TelemetryDeck App ID
TELEMETRY_DECK_APP_ID = $(TELEMETRY_DECK_APP_ID)

// WishKit API key
WISHKIT_API_KEY = $(WISHKIT_API_KEY)

// AIProxy DeviceCheck Bypass
AIPROXY_DEVICE_CHECK_BYPASS = $(AIPROXY_DEVICE_CHECK_BYPASS)

// App version information
// CI_VERSION_SUFFIX is set by ci_post_clone.sh: empty for releases, -pr.XXX for PR builds (semver pre-release)
MARKETING_VERSION = 2.2.0$(CI_VERSION_SUFFIX)
